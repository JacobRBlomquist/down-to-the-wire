{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths":{
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@public/*": ["public/*"],
      "@layouts/*": ["src/layouts/*"],
      "@src/*":["src/*"]
    }
  },
}
